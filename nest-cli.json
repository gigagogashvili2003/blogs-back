{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "assets": [
      "./libs/notifications-lib/src/templates/**/*"
    ],
    "watchAssets": true
  },
  "projects": {
